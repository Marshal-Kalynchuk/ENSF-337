#include<stdio.h>

int main()
{
int inty = 1;
char chary = 'C';
float floaty;
printf("\nThis is an integer: %d\n\nThis is a char: %c\n\n", inty, chary);
printf("Please enter a float:");
scanf("%f", &floaty);
if (inty > floaty) {
	printf("\nFloat is less then %d\n\n", inty);
} else {
	printf("\nFloat is greater then %d\n\n", inty);
}

}
